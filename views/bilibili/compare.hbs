<h1>{{title}}</h1>
{{#if msg}}
    <p>{{msg}}</p>
{{/if}}
{{#if file}}
    <p>本地文件：{{file}}</p>
{{/if}}
{{#if add.length}}
    <h2>服务端多出条目</h2>
    <ul>
        {{#each add}}<li>{{_id}} - {{_title}}</li>{{/each}}
    </ul>
{{/if}}
{{#if remove.length}}
    <h2>本地文件多出条目</h2>
    <ul>
        {{#each remove}}<li>{{_id}} - {{_title}}</li>{{/each}}
    </ul>
{{/if}}